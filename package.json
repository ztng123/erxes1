{
  "name": "erxes-widgets",
  "version": "0.9.1",
  "description": "Embedable widget scripts server for erxes",
  "homepage": "https://erxes.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/erxes/erxes-widgets"
  },
  "bugs": "https://github.com/erxes/erxes-widgets/issues",
  "keywords": [
    "node",
    "express",
