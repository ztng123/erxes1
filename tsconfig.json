{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strictNullChecks": true,
        "module": "commonjs",
         "lib": [
          "es5",
          "es6",
          "dom",
          "esnext.asynciterable"
        ],
        "jsx": "react",
        "target": "es5",
        "allowJs": true
    },
    "awesomeTypescriptLoaderOptions": {
      "useWebpackText": true,
      "useTranspileModule": true,
      "doTypeCheck": true,
      "forkChecker": true
    },
    "exclude": ["node_modules", "dist"]
}
